config BR2_PACKAGE_APP_OPENCV_RESIZE
    bool "APP_OPENCV_DEMO_0"
    depends on BR2_PACKAGE_OPENCV4
    depends on BR2_PACKAGE_OPENCV4_LIB_IMGPROC
    help
        APP_OPENCV_DEMO_0 is an demo package for use opencv.


config BR2_PACKAGE_APP_OPENCV_RESIZE_LOG_LEVEL
    string "Log level for app_opencv_resize"
    default "LOG_LEVEL_INFO"
    help
        Set the log level for app_opencv_resize. Possible values are:
        LOG_LEVEL_INFO, LOG_LEVEL_DEBUG, etc.


config BR2_PACKAGE_APP_OPENCV_RESIZE_BUILD_TYPE
    string "Build type for app_opencv_resize"
    default "Release"
    help
        Set the build type for app_opencv_resize. Possible values are:
        Debug, Release.